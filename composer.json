{
    "name": "wakqasahmed/laravel-azure-servicebus-queue",
    "description": "Extending the Laravel queue to support the Microsoft Azure Service Bus Queue.",
    "keywords": [
        "laravel",
        "azure",
        "servicebus",
        "queue"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Bouma",
            "email": "me@alexbouma.me"
        },
        {
            "name": "Waqas Ahmed",
            "email": "info@wakqasahmed.com"
        }
    ],
    "require": {
        "php": ">=7.0.10",
        "illuminate/container": "5.5.*",
        "illuminate/support": "5.5.*",
        "illuminate/encryption": "5.5.*",
        "microsoft/windowsazure": "0.5.7"
    },
    "replace": {
        "illuminate/container": "self.version",
        "illuminate/support": "self.version",
        "illuminate/encryption": "self.version"        
    },    
    "autoload": {
        "psr-0": {
            "Stayallive\\LaravelAzureServicebusQueue\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
